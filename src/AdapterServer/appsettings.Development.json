{
  "DetailedErrors": true,
  "ConnectionStrings": {
    "DefaultConnection": "Data/adapter_shell.db",
    "HangfireConnection": "Data/hangfire.db",
    "CIRLibConnection": "Data/CIRLib.db",
    "NotificationsConnection": "Data/notifications.db"
  },
  "Hangfire": {
    "Storage": "SQLiteStorage"
  },
  "Isbm:ListenerUrlBase": "http://host.docker.internal:5060/",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AuthenticationExtesion.AWS.LoadBalancerAuthenticationHandler": "Trace"
    }
  },
  "Authentication": {
    "Schemes": {
      "AwsLoadBalancerAuthentication": {
        "AwsRegion": "ap-southeast-2",
        "LoadBalancerPublicKeyUri": "../AuthenticationExtensionJwtGen/claims_pub_key.pem",
        "IdPJwksUri": "../AuthenticationExtensionJwtGen/access_jwks.json",
        "ValidAudiences": [
          "http://localhost:20453",
          "https://localhost:44320",
          "https://localhost:7074",
          "http://localhost:5060",
          "fake-audience"
        ],
        "ValidIssuersForAccessToken": [
          "http://localhost/claimsdata",
          "http://localhost/tokens/00000000-0000-0000-0000-000000000002/v2.0"
        ],
        "ValidIssuersForClaimsToken": "http://localhost/claimsdata",
        "ApplicationId": "00000000-0000-0000-0000-000000000001",
        "TenantId": "00000000-0000-0000-0000-000000000002"
      }
    }
  }
}
